{
    "name": "pvz-endpoint-tester",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "build": "tsc -b && vite build",
        "build:dev": "tsc -b && vite build --mode dev",
        "build:prod": "tsc -b && vite build --mode prod",
        "build:rec": "tsc -b && vite build --mode rec",
        "deadcode": "knip --exports",
        "preview": "vite preview",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "start": "vite",
        "start:dev": "vite --mode dev",
        "start:localdev": "vite --mode localdev",
        "start:prod": "vite --mode prod",
        "start:rec": "vite --mode rec",
        "test": "vitest run"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@headlessui/react": "^1.7.18",
        "@mui/icons-material": "^6.4.1",
        "@mui/material": "^6.4.7",
        "@tanstack/react-query": "^5.65.1",
        "axios": "^1.6.7",
        "lucide-react": "^0.344.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.22.3"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.1",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "autoprefixer": "^10.4.21",
        "eslint": "^9.9.1",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "eslint-plugin-react-refresh": "^0.4.11",
        "globals": "^15.9.0",
        "postcss": "^8.5.3",
        "sass-embedded": "^1.83.4",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.3.0",
        "vite": "^5.4.2"
    }
}
